    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.8",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.3",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.17"
  }
