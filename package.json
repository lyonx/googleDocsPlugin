  "description": "A plugin that provides integration with google apps doc to allow users to view the google doc sheet.",
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
